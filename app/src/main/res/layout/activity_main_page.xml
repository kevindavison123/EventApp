                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitEnd"
                android:layout_weight="0.5"
                android:src="@drawable/compton" />
        </TableRow>
        <TableRow
            android:id="@+id/row3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/event5"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitStart"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2"
                android:onClick="onUserClickConcept"/>
            <ImageButton
                android:id="@+id/event6"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="right"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitEnd"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2" />
        </TableRow>
        <TableRow
            android:id="@+id/row4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/event7"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitStart"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2"
                android:onClick="onUserClickConcept"/>
            <ImageButton
                android:id="@+id/event8"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="right"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitEnd"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2" />
        </TableRow>
        <TableRow
            android:id="@+id/row5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/event9"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitStart"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2"
                android:onClick="onUserClickConcept"/>
            <ImageButton
                android:id="@+id/event10"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="right"
                android:adjustViewBounds="true"
                android:cropToPadding="true"
                android:scaleType="fitEnd"
                android:layout_weight="0.5"
                android:src="@drawable/uncle2" />
        </TableRow>


    </TableLayout>
